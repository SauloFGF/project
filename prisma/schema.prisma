// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id                Int    @id @default(autoincrement())
  funcionario       String
  funcao            String
  atividade         String
  quantidadeS       Int
  peso              Float
  quantidade_Palets Int

  @@map("Produtividade")
}
